# Production Environment Variables
NODE_ENV=production

# API Configuration
REACT_APP_API_URL=https://api.regalytics.com/api
REACT_APP_WS_URL=wss://api.regalytics.com/ws
REACT_APP_USE_REAL_API=true

# Authentication
REACT_APP_AUTH_DOMAIN=regalytics.auth0.com
REACT_APP_AUTH_CLIENT_ID=your_auth0_client_id
REACT_APP_AUTH_AUDIENCE=https://api.regalytics.com

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
REACT_APP_SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# Security
REACT_APP_CSP_NONCE=your_csp_nonce
REACT_APP_HASH_ROUTING=true

# Performance
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_CACHE_VERSION=1.0.0

# Monitoring
REACT_APP_ENABLE_METRICS=true
REACT_APP_METRICS_ENDPOINT=/metrics
